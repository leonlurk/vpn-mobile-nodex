# Configuraci√≥n del Servidor VPN Nodex
NODE_ENV=development
PORT=3000
VPN_TCP_PORT=8443
VPN_UDP_PORT=8444

# Firebase Configuration
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_PRIVATE_KEY_PATH=./config/firebase-admin.json

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
