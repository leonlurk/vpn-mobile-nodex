# Server Configuration
SERVER_IP=92.113.32.217
ALLOWED_ORIGINS=*
LOG_LEVEL=debug

# VPN Configuration
MAX_CONCURRENT_CONNECTIONS=100
CONNECTION_TIMEOUT=30000
